Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Sigma]R", "=", 
   RowBox[{"48", " ", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "2"}], "\[Xi]", " ", 
      RowBox[{"L", "/", 
       SqrtBox[
        RowBox[{"Du", "/", "2"}]]}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Sigma]D", "=", 
   RowBox[{
    RowBox[{"8", 
     FractionBox["Dv", 
      RowBox[{
       SqrtBox[
        RowBox[{"Du", "/", "2"}]], " ", "L", 
       RowBox[{"(", 
        RowBox[{"1", "-", "\[Xi]"}], ")"}]}]], 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "2"}], "\[Xi]", " ", 
       RowBox[{"L", "/", 
        SqrtBox[
         RowBox[{"Du", "/", "2"}]]}]}], "]"}]}], "/.", 
    RowBox[{"\[Xi]", "\[Rule]", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"nbar", "+", "1"}], ")"}], "/", "2"}]}]}]}], ";"}]}], "Input",E\
