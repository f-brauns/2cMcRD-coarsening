Cell[CellGroupData[{

Cell["Setup: function to locate fold by continuation in \[Epsilon]", \
"Subsection",ExpressionUUID->"cfc4ee48-469e-4183-a2ce-12bbb33b1113"],

Cell[BoxData[
 RowBox[{
  RowBox[{"findFold", "[", 
   RowBox[{"params_", ",", 
    RowBox[{"{", 
     RowBox[{"epsInit_", ",", "epsMax_"}], "}"}]}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"initRun", ",", "initSol", ",", "contData"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"initRun", "=", 
      RowBox[{"runTwoComponentSim", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"f", ",", "g"}], "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{"Join", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"eps", "\[Rule]", "epsInit"}], "}"}], ",", "params"}], 
         "]"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"3", 
              SqrtBox[
               RowBox[{"d", "/", "2"}]]}], "+", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"1", "-", "d"}], ")"}], 
              RowBox[{
               SqrtBox[
                RowBox[{"2", "/", "d"}]], "/", "2"}], 
              RowBox[{"(", 
               RowBox[{"1", "-", 
                RowBox[{"Tanh", "[", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"#", "-", 
                    RowBox[{"L", " ", "xi"}]}], ")"}], "/", 
                  RowBox[{"(", 
                   RowBox[{"2", 
                    SqrtBox["Du"]}], ")"}]}], "]"}]}], ")"}]}]}], "&"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"3", 
             SqrtBox[
              RowBox[{"d", "/", "2"}]]}], "&"}]}], "\[IndentingNewLine]", 
          "}"}], "/.", 
         RowBox[{"xi", "\[Rule]", 
          RowBox[{"p", "/", 
           SqrtBox[
            RowBox[{"2", "/", "d"}]]}]}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"T", "/.", "params"}]}], "\[IndentingNewLine]", "]"}]}], ";", 
     "\[IndentingNewLine]", 
