Cell["Quadratic interpolation to find fold location", "Subsection",ExpressionUUID->"fd9319a4-fb5b-4156-9ca7-60f05df7b591"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{"fit", "=", 
      RowBox[{"First", "@", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"f", "[", "x1", "]"}], "\[Equal]", "y1"}], ",", 
            RowBox[{
             RowBox[{"f", "[", "x2", "]"}], "\[Equal]", "y2"}], ",", 
            RowBox[{
             RowBox[{"f", "[", "x3", "]"}], "\[Equal]", "y3"}]}], "}"}], "/.", 
          RowBox[{
           RowBox[{"f", "[", "x_", "]"}], "\[RuleDelayed]", 
           RowBox[{
            RowBox[{"a", " ", 
             RowBox[{"x", "^", "2"}]}], "+", 
            RowBox[{"b", " ", "x"}], "+", "c"}]}]}], ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "b", ",", "c"}], "}"}]}], "]"}]}]}], 
     "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"fitParabola", "[", 
      RowBox[{"p", ":", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"_", ",", "_"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"_", ",", "_"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"_", ",", "_"}], "}"}]}], "}"}]}], "]"}], ":=", 
     RowBox[{"fit", "/.", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"x1", "\[Rule]", 
         RowBox[{"p", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ",", 
        RowBox[{"x2", "\[Rule]", 
         RowBox[{"p", "[", 
          RowBox[{"[", 
           RowBox[{"2", ",", "1"}], "]"}], "]"}]}], ",", 
        RowBox[{"x3", "\[Rule]", 
         RowBox[{"p", "[", 
          RowBox[{"[", 
           RowBox[{"3", ",", "1"}], "]"}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"y1", "\[Rule]", 
         RowBox[{"p", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "2"}], "]"}], "]"}]}], ",", 
        RowBox[{"y2", "\[Rule]", 
         RowBox[{"p", "[", 
          RowBox[{"[", 
           RowBox[{"2", ",", "2"}], "]"}], "]"}]}], ",", 
        RowBox[{"y3", "\[Rule]", 
         RowBox[{"p", "[", 
          RowBox[{"[", 
           RowBox[{"3", ",", "2"}], "]"}], "]"}]}]}], "\[IndentingNewLine]", 
       "}"}]}]}]}], "\[IndentingNewLine]", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{"parabolaMaximum", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"x", ",", 
         RowBox[{
          RowBox[{"a", " ", 
           RowBox[{"x", "^", "2"}]}], "+", 
          RowBox[{"b", " ", "x"}], "+", "c"}]}], "}"}], "/.", 
       RowBox[{"First", "@", 
        RowBox[{"Solve", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"D", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"a", " ", 
               RowBox[{"x", "^", "2"}]}], "+", 
              RowBox[{"b", " ", "x"}], "+", "c"}], ",", "x"}], "]"}], 
           "\[Equal]", "0"}], ",", "x"}], "]"}]}]}]}], "\[IndentingNewLine]", 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"quadraticInterpolateMaximum", "[", 
      RowBox[{"points", ":", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"_", ",", "_"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"_", ",", "_"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"_", ",", "_"}], "}"}]}], "}"}]}], "]"}], ":=", 
     RowBox[{"parabolaMaximum", "/.", 
      RowBox[{"fitParabola", "@", "points"}]}]}]}], "\[IndentingNewLine]", 
   "]"}], ";"}]}], "Input",
